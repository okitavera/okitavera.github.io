{% for post in articles | reverse %}
  <div class="article-item">
    <time class="hashed uppercased fontsize-min1" datetime="{{ post.date | htmlDateString }}"><b>{{ post.date | readableDate }}</b></time><br/>
    <a href="{{ post.url | url }}">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
  </div>
  <div class="sp sp-xs"></div>
{% endfor %}
